	
{
	"name": "${apiName}",
	"path": "${apiPath}",
	"state": "${status}",
	"version": "1.0.0",
	"organization": "API Development ${orgNumber}",
    "securityProfiles": [
     	{
     		"name": "_default",
     		"isDefault": true,
     		"devices": [
     			{
     				"name": "API Key",
     				"type": "apiKey",
     				"order": 0,
     				"properties": {
     					"apiKeyFieldName": "${apiKeyFieldName}",
     					"takeFrom": "${takeFrom}",
     					"removeCredentialsOnSuccess": "${removeCredentialsOnSuccess}"
     				}
     			}
     		]
     	}
     ]
}