{
  "config": {
    "portalName": "${portalName}",
    "portalHostname": "localhost",
    "apiPortalName": "Axway API Portal",
    "apiPortalHostname": "localhost",
    "isApiPortalConfigured": true,
    "registrationEnabled": true,
    "resetPasswordEnabled": true,
    "changePasswordOnFirstLogin": false,
    "passwordExpiryEnabled": false,
    "passwordLifetimeDays": 90,
    "minimumPasswordLength": 1,
    "autoApproveUserRegistration": true,
    "systemOAuthScopesEnabled": false,
    "applicationScopeRestrictions": false,
    "autoApproveApplications": true,
    "delegateUserAdministration": true,
    "delegateApplicationAdministration": true,
    "apiDefaultVirtualHost": "api-env.demo.axway.com:8065",
    "apiRoutingKeyEnabled": true,
    "apiRoutingKeyLocation": "query|v",
    "emailFrom": "no-reply@axway.com",
    "emailBounceAddress": "apiadmin@localhost",
    "promoteApiViaPolicy": false,
    "globalPoliciesEnabled": false,
    "faultHandlersEnabled": true,
    "globalFaultHandlerPolicy": "Default Fault Handler",
    "strictCertificateChecking": true,
    "serverCertificateVerification": true,
    "advisoryBannerEnabled": false,
    "advisoryBannerText": "Warning - unauthorized use of this tool is strictly prohibited and subject to audit, investigation, and potential prosecution.",
    "sessionIdleTimeout": 3600000,
    "loginNameRegex": "[^;,\\\\/?#<>&;!]{1,}",
    "userNameRegex": "^[\\p{L}\\d .,'_-]+$",
    "isTrial": false,
    "defaultTrialDuration": 30,
    "apiImportTimeout": 60,
    "apiImportMimeValidation": true,
    "apiImportEditable": true,
    "lockUserAccount": {
      "enabled": true,
      "attempts": 6,
      "timePeriod": 5,
      "timePeriodUnit": "minute",
      "lockTimePeriod": 30,
      "lockTimePeriodUnit": "minute"
    }
  }
}