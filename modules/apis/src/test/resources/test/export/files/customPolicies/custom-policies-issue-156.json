{
  "name": "${apiName}",
  "path": "${apiPath}",
  "state": "${state}",
  "version": "v1",
  "organization": "API Development ${orgNumber}",
  "securityProfiles": [
    {
      "name": "_default",
      "isDefault": true,
      "devices": [
        {
          "name": "OAuth (External)",
          "type": "oauthExternal",
          "order": 1,
          "properties": {
            "tokenStore": "${tokenInfoPolicy}",
            "accessTokenLocation": "HEADER",
            "authorizationHeaderPrefix": "Bearer",
            "accessTokenLocationQueryString": "",
            "scopesMustMatch": "Any",
            "scopes": "1.0",
            "removeCredentialsOnSuccess": "false",
            "implicitGrantEnabled": "true",
            "implicitGrantLoginEndpointUrl": "https://login.microsoftonline.com/5983457345783489759834753/oauth2/authorize",
            "implicitGrantLoginTokenName": "access_token",
            "authCodeGrantTypeEnabled": "true",
            "authCodeGrantTypeRequestEndpointUrl": "https://login.microsoftonline.com/5983457345783489759834753/oauth2/authorize",
            "authCodeGrantTypeRequestClientIdName": "client_id",
            "authCodeGrantTypeRequestSecretName": "client_secret",
            "authCodeGrantTypeTokenEndpointUrl": "https://login.microsoftonline.com/5983457345783489759834753/oauth2/token",
            "authCodeGrantTypeTokenEndpointTokenName": "access_code",
            "useClientRegistry": "true",
            "subjectSelector": "${//oauth.token.client_id//}",
            "oauth.token.client_id": "${//oauth.token.client_id//}",
            "oauth.token.scopes": "${//oauth.token.scopes//}",
            "oauth.token.valid": "${//oauth.token.valid//}"
          }
        }
      ]
    }
  ],
  "outboundProfiles": {
    "_default": {
      "authenticationProfile": "_default",
      "requestPolicy": "${requestPolicy}",
      "responsePolicy": "${responsePolicy}"
    }
  },
  "caCerts" : [ {
    "certFile" : "sample-certificate.crt",
    "inbound" : "false",
    "outbound" : "true"
  }, {
    "certFile" : "Sample'EncryptAuthority.crt",
    "inbound" : "false",
    "outbound" : "true"
  }, {
    "certFile" : "SampleRootCA.crt",
    "inbound" : "false",
    "outbound" : "true"
  } ],
  "tags": {
    "ping_ai": [
      "true"
    ],
    "ping_blocking": [
      "true"
    ]
  }
}
