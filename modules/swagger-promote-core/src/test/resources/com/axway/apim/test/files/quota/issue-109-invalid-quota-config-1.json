{
   "name": "${apiName}",
   "path": "${apiPath}",
   "state": "${state}",
   "version": "1.0.0",
   "organization": "API Development ${orgNumber}",
   "applicationQuota": {
      "restrictions": [
         {
            "method": "*",
            "type": "throttle",
            "config": {
               "period": "seconds",
               "messages": 25,
               "per": 60
            }
         }
      ]
   }
}